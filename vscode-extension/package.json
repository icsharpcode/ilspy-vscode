{
	"name": "ilspy-vscode",
	"displayName": "ilspy-vscode",
	"description": "Decompile MSIL assemblies - support for full framework, .NET Core and .NET Standard",
	"version": "0.17.0-model-tools.1",
	"icon": "resources/ILSpy-vscode-marketplace-icon.png",
	"publisher": "icsharpcode",
	"preview": true,
	"private": true,
	"license": "SEE LICENSE",
	"homepage": "https://github.com/icsharpcode/ilspy-vscode/tree/master/vscode-extension",
	"bugs": "https://github.com/icsharpcode/ilspy-vscode/issues",
	"repository": {
		"type": "git",
		"url": "https://github.com/icsharpcode/ilspy-vscode"
	},
	"engines": {
		"vscode": "^1.101.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"configuration": [
			{
				"title": "ILSpy",
				"properties": {
					"ilspy.defaultOutputLanguage": {
						"description": "Defines to which language or language variant ILSpy will decompile assemblies by default. Note that you can choose a different language for a specific view later.",
						"type": "string",
						"default": "C# 12.0 / VS 2022.8",
						"enum": [
							"C# 12.0 / VS 2022.8",
							"C# 11.0 / VS 2022.4",
							"C# 10.0 / VS 2022",
							"C# 9.0 / VS 2019.8",
							"C# 8.0 / VS 2019",
							"C# 7.3 / VS 2017.7",
							"C# 7.2 / VS 2017.4",
							"C# 7.1 / VS 2017.3",
							"C# 7.0 / VS 2017",
							"C# 6.0 / VS 2015",
							"C# 5.0 / VS 2012",
							"C# 4.0 / VS 2010",
							"C# 3.0 / VS 2008",
							"C# 2.0 / VS 2005",
							"C# 1.0 / VS .NET",
							"IL"
						],
						"enumDescriptions": [
							"Decompile to C# 12.0 by default",
							"Decompile to C# 11.0 by default",
							"Decompile to C# 10.0 by default",
							"Decompile to C# 9.0 by default",
							"Decompile to C# 8.0 by default",
							"Decompile to C# 7.3 by default",
							"Decompile to C# 7.2 by default",
							"Decompile to C# 7.1 by default",
							"Decompile to C# 7.0 by default",
							"Decompile to C# 6.0 by default",
							"Decompile to C# 5.0 by default",
							"Decompile to C# 4.0 by default",
							"Decompile to C# 3.0 by default",
							"Decompile to C# 2.0 by default",
							"Decompile to C# 1.0 by default",
							"Decompile to readable IL instructions by default"
						]
					},
					"ilspy.loadPreviousAssemblies": {
						"description": "If activated, the list of loaded assemblies is stored with current workspace and is restored on re-opening. This doesn't apply for temporarily auto-loaded dependencies.",
						"type": "boolean",
						"default": "true"
					},
					"ilspy.autoLoadDependencies": {
						"description": "If activated, dependency assemblies are temporarily added to assembly tree on demand.",
						"type": "boolean",
						"default": "true"
					},
					"ilspy.showCompilerGeneratedSymbols": {
						"description": "If activated, shows compiler-generated symbols.",
						"type": "boolean",
						"default": "false"
					}
				}
			}
		],
		"viewsContainers": {
			"panel": [
				{
					"id": "ilspySearchResultsContainer",
					"title": "ILSpy: Search Results",
					"icon": "resources/il-spy-modern-icon.svg"
				},
				{
					"id": "ilspyAnalyzeResultsContainer",
					"title": "ILSpy: Analyze",
					"icon": "resources/il-spy-modern-icon.svg"
				}
			]
		},
		"views": {
			"explorer": [
				{
					"id": "ilspyDecompiledMembers",
					"name": "ILSpy: Assemblies",
					"when": "ilspy.backendAvailable",
					"icon": "resources/il-spy-modern-icon.svg"
				}
			],
			"ilspySearchResultsContainer": [
				{
					"id": "ilspySearchResultsContainer",
					"name": "ILSpy: Search Results",
					"when": "ilspy.searchResultsToShow",
					"icon": "resources/il-spy-modern-icon.svg"
				}
			],
			"ilspyAnalyzeResultsContainer": [
				{
					"id": "ilspyAnalyzeResultsContainer",
					"name": "ILSpy: Analyze",
					"when": "ilspy.analyzeResultsToShow",
					"icon": "resources/il-spy-modern-icon.svg"
				}
			]
		},
		"viewsWelcome": [
			{
				"view": "ilspyDecompiledMembers",
				"contents": "No assemblies selected yet\n[Add assembly from workspace](command:ilspy.decompileAssemblyInWorkspace)\n[Pick assembly from file system](command:ilspy.decompileAssemblyViaDialog)\n\nGet [more help](https://github.com/icsharpcode/ilspy-vscode/wiki/Getting-started#usage)",
				"when": "ilspy.backendAvailable"
			}
		],
		"languageModelTools": [
			{
				"name": "ilspy_addAssembly",
				"displayName": "Load Assembly for Decompilation",
				"canBeReferencedInPrompt": true,
				"toolReferenceName": "load-assembly",
				"icon": "$(package)",
				"tags": [
					"decompilation",
					"dotnet",
					"csharp",
					"assembly"
				],
				"userDescription": "Load a .NET assembly (.dll, .exe) for decompilation and exploration",
				"modelDescription": "Loads a .NET assembly file for decompilation. Once loaded, symbols can be searched and decompiled. Required before searching or decompiling.",
				"inputSchema": {
					"type": "object",
					"properties": {
						"assemblyPath": {
							"type": "string",
							"description": "Absolute path to the assembly file (.dll, .exe, .winmd, .netmodule)"
						}
					},
					"required": [
						"assemblyPath"
					]
				}
			},
			{
				"name": "ilspy_searchAssemblies",
				"displayName": "Search Loaded Assemblies",
				"canBeReferencedInPrompt": true,
				"toolReferenceName": "search-assemblies",
				"icon": "$(search)",
				"tags": [
					"decompilation",
					"dotnet",
					"csharp",
					"search"
				],
				"userDescription": "Search for types, methods, properties, etc. across all loaded assemblies",
				"modelDescription": "Searches for symbols across loaded assemblies. Returns results with URIs for decompilation. Assemblies must be loaded first with ilspy_addAssembly.",
				"inputSchema": {
					"type": "object",
					"properties": {
						"searchTerm": {
							"type": "string",
							"description": "Search term (e.g., 'Http', 'Parse', 'System.String')"
						}
					},
					"required": [
						"searchTerm"
					]
				}
			},
			{
				"name": "ilspy_decompileNode",
				"displayName": "Decompile Symbol to Source Code",
				"canBeReferencedInPrompt": true,
				"toolReferenceName": "decompile-symbol",
				"icon": "$(file-code)",
				"tags": [
					"decompilation",
					"dotnet",
					"csharp",
					"source-code"
				],
				"userDescription": "Decompile a specific type, method, property, etc. to readable C# source code",
				"modelDescription": "Decompiles a symbol to C# source code. Takes ILSpy URI from search results. Returns decompiled code. Referenced symbols can be recursively decompiled.",
				"inputSchema": {
					"type": "object",
					"properties": {
						"uri": {
							"type": "string",
							"description": "ILSpy URI of the symbol to decompile (obtained from search results, format: ilspy://...)"
						},
						"outputLanguage": {
							"type": "string",
							"description": "Output language version (default: 'C# 12.0 / VS 2022.8')",
							"enum": [
								"C# 12.0 / VS 2022.8",
								"C# 11.0 / VS 2022.4",
								"C# 10.0 / VS 2022",
								"C# 9.0 / VS 2019.8",
								"C# 8.0 / VS 2019",
								"IL"
							]
						}
					},
					"required": [
						"uri"
					]
				}
			},
			{
				"name": "ilspy_getLoadedAssemblies",
				"displayName": "List Loaded Assemblies",
				"canBeReferencedInPrompt": true,
				"toolReferenceName": "list-assemblies",
				"icon": "$(library)",
				"tags": [
					"decompilation",
					"dotnet",
					"csharp",
					"assembly"
				],
				"userDescription": "Get a list of all currently loaded assemblies available for decompilation",
				"modelDescription": "Returns list of loaded assemblies with names, paths, and URIs.",
				"inputSchema": {
					"type": "object",
					"properties": {}
				}
			},
			{
				"name": "ilspy_navigateDefinition",
				"displayName": "Navigate to Symbol Definition",
				"canBeReferencedInPrompt": true,
				"toolReferenceName": "navigate-definition",
				"icon": "$(go-to-file)",
				"tags": [
					"decompilation",
					"dotnet",
					"csharp",
					"navigation"
				],
				"userDescription": "Navigate from a code position to its definition, automatically decompiling if needed",
				"modelDescription": "Go to definition from a document position. Automatically decompiles if definition is in an external assembly. Enables deep code navigation.",
				"inputSchema": {
					"type": "object",
					"properties": {
						"documentUri": {
							"type": "string",
							"description": "URI of the document containing the symbol reference (can be file://, ilspy://, or other schemes)"
						},
						"line": {
							"type": "number",
							"description": "Line number (0-based) where the symbol is referenced"
						},
						"character": {
							"type": "number",
							"description": "Character position (0-based) on the line where the symbol starts"
						}
					},
					"required": [
						"documentUri",
						"line",
						"character"
					]
				}
			},
			{
				"name": "ilspy_openDecompiledSource",
				"displayName": "Open Decompiled Source in Editor",
				"canBeReferencedInPrompt": true,
				"toolReferenceName": "open-source",
				"icon": "$(file-code)",
				"tags": [
					"decompilation",
					"dotnet",
					"csharp",
					"ui",
					"editor"
				],
				"userDescription": "Opens the decompiled source code for a symbol in the VS Code editor",
				"modelDescription": "Opens decompiled source in editor tab. Use when user wants to view/explore code themselves. Prefer over ilspy_decompileNode for viewing intent.",
				"inputSchema": {
					"type": "object",
					"properties": {
						"uri": {
							"type": "string",
							"description": "ILSpy URI of the symbol to open (obtained from search results, format: ilspy://...)"
						}
					},
					"required": [
						"uri"
					]
				}
			},
			{
				"name": "ilspy_openAnalyzePanel",
				"displayName": "Open Symbol Analysis Panel",
				"canBeReferencedInPrompt": true,
				"toolReferenceName": "analyze-symbol",
				"icon": "$(graph)",
				"tags": [
					"decompilation",
					"dotnet",
					"csharp",
					"ui",
					"analysis"
				],
				"userDescription": "Opens the ILSpy Analyze panel showing where and how a symbol is used",
				"modelDescription": "Opens ILSpy Analyze panel showing: Used By, Instantiated By, Overridden By, and other relationships. Use for understanding usage patterns.",
				"inputSchema": {
					"type": "object",
					"properties": {
						"uri": {
							"type": "string",
							"description": "ILSpy URI of the symbol to analyze (obtained from search results, format: ilspy://...)"
						}
					},
					"required": [
						"uri"
					]
				}
			},
			{
				"name": "ilspy_searchAndOpen",
				"displayName": "Search and Open Best Match",
				"canBeReferencedInPrompt": true,
				"toolReferenceName": "quick-open",
				"icon": "$(search-view-icon)",
				"tags": [
					"decompilation",
					"dotnet",
					"csharp",
					"ui",
					"search"
				],
				"userDescription": "Searches for a symbol and immediately opens the most relevant match in the editor",
				"modelDescription": "Searches and opens best match in one step. Prefers exact names and top-level types. Use for 'show me X' queries.",
				"inputSchema": {
					"type": "object",
					"properties": {
						"searchTerm": {
							"type": "string",
							"description": "Name of the symbol to search for and open (e.g., 'Stopwatch', 'HttpClient', 'Task')"
						}
					},
					"required": [
						"searchTerm"
					]
				}
			}
		],
		"commands": [
			{
				"command": "ilspy.decompileSelectedAssembly",
				"title": "Decompile selected assembly",
				"category": "ILSpy"
			},
			{
				"command": "ilspy.decompileAssemblyInWorkspace",
				"title": "Add assembly from workspace",
				"category": "ILSpy"
			},
			{
				"command": "ilspy.decompileAssemblyViaDialog",
				"title": "Pick assembly from file system",
				"category": "ILSpy"
			},
			{
				"command": "ilspy.unloadAssembly",
				"title": "Unload Assembly",
				"category": "ILSpy",
				"icon": "$(close)"
			},
			{
				"command": "ilspy.reloadAssembly",
				"title": "Reload Assembly",
				"category": "ILSpy",
				"icon": "$(refresh)"
			},
			{
				"command": "ilspy.selectOutputLanguage",
				"title": "Select output language",
				"category": "ILSpy"
			},
			{
				"command": "ilspy.search",
				"title": "Search in Decompiled Assemblies",
				"category": "ILSpy",
				"icon": "$(search-view-icon)"
			},
			{
				"command": "ilspy.searchEditorSelection",
				"title": "Search with ILSpy",
				"category": "ILSpy",
				"icon": "$(search-view-icon)"
			},
			{
				"command": "ilspy.analyze",
				"title": "Analyze Element",
				"category": "ILSpy",
				"icon": "$(search-view-icon)"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "ilspy.decompileSelectedAssembly",
					"group": "navigation",
					"when": "resourceExtname == .dll || resourceExtname == .exe || resourceExtname == .winmd || resourceExtname == .netmodule || resourceExtname == .wasm"
				}
			],
			"editor/context": [
				{
					"command": "ilspy.searchEditorSelection",
					"when": "ilspy.backendAvailable && ilspy.treeWithNodes"
				}
			],
			"view/title": [
				{
					"submenu": "ilspy/view/assemblytree/add",
					"when": "view == ilspyDecompiledMembers",
					"group": "navigation"
				},
				{
					"command": "ilspy.search",
					"when": "view == ilspyDecompiledMembers && ilspy.treeWithNodes",
					"group": "navigation"
				}
			],
			"ilspy/view/assemblytree/add": [
				{
					"command": "ilspy.decompileAssemblyInWorkspace",
					"when": "view == ilspyDecompiledMembers"
				},
				{
					"command": "ilspy.decompileAssemblyViaDialog",
					"when": "view == ilspyDecompiledMembers"
				}
			],
			"view/item/context": [
				{
					"command": "ilspy.unloadAssembly",
					"when": "view == ilspyDecompiledMembers && viewItem == assemblyNode",
					"group": "inline"
				},
				{
					"command": "ilspy.reloadAssembly",
					"when": "view == ilspyDecompiledMembers && viewItem == assemblyNode",
					"group": "inline"
				},
				{
					"command": "ilspy.analyze",
					"when": "(view == ilspyDecompiledMembers || view == ilspySearchResultsContainer || view == ilspyAnalyzeResultsContainer) && viewItem == analyzableNode",
					"group": "inline"
				}
			],
			"commandPalette": [
				{
					"command": "ilspy.decompileAssemblyInWorkspace",
					"when": "ilspy.backendAvailable"
				},
				{
					"command": "ilspy.decompileAssemblyViaDialog",
					"when": "ilspy.backendAvailable"
				},
				{
					"command": "ilspy.selectOutputLanguage",
					"when": "resourceScheme == ilspy"
				},
				{
					"command": "ilspy.search",
					"when": "ilspy.backendAvailable && ilspy.treeWithNodes"
				},
				{
					"command": "ilspy.decompileSelectedAssembly",
					"when": "false"
				},
				{
					"command": "ilspy.searchEditorSelection",
					"when": "false"
				},
				{
					"command": "ilspy.analyze",
					"when": "false"
				},
				{
					"command": "ilspy.reloadAssembly",
					"when": "false"
				},
				{
					"command": "ilspy.unloadAssembly",
					"when": "false"
				}
			]
		},
		"submenus": [
			{
				"id": "ilspy/view/assemblytree/add",
				"label": "Add Assembly",
				"icon": "$(add)"
			}
		],
		"colors": [
			{
				"id": "ilspy.autoLoadedAssemblyTextForeground",
				"description": "Text color of auto-loaded assemblies",
				"defaults": {
					"dark": "#4682B4",
					"light": "#4682B4",
					"highContrast": "#4682B4"
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile": "webpack --mode development",
		"watch": "webpack --mode development --watch",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"dependencies": {
		"ansi-regex": "6.2.2",
		"brace-expansion": "4.0.1",
		"chai": "6.2.0",
		"os": "0.1.2",
		"path": "0.12.7",
		"temp-dir": "3.0.0",
		"terser": "5.44.0"
	},
	"devDependencies": {
		"@types/chai": "5.2.2",
		"@types/glob": "9.0.0",
		"@types/mocha": "10.0.10",
		"@types/node": "22.15.31",
		"@types/vscode": "1.101.0",
		"@typescript-eslint/eslint-plugin": "8.46.1",
		"@typescript-eslint/parser": "8.46.1",
		"@vscode/test-electron": "2.5.2",
		"@vscode/vsce": "3.6.2",
		"eslint": "9.38.0",
		"glob": "11.0.3",
		"minimist": "1.2.8",
		"mocha": "11.7.4",
		"terser-webpack-plugin": "5.3.14",
		"ts-loader": "9.5.4",
		"typescript": "5.9.3",
		"webpack": "5.102.1",
		"webpack-cli": "6.0.1",
		"vscode-languageclient": "9.0.1"
	},
	"extensionDependencies": [
		"ms-dotnettools.vscode-dotnet-runtime"
	]
}
